<script>
        //Summation
        $("#CalculateTotalScore").click(function () {
            var a = parseInt($("#target1").val()) || 0;
            var b = parseInt($("#target2").val()) || 0;
            var c = parseInt($("#target3").val()) || 0;
            var d = parseInt($("#target4").val()) || 0;
            var e = parseInt($("#target5").val()) || 0;
            var f = parseInt($("#target6").val()) || 0;
            var g = parseInt($("#target7").val()) || 0;
            var h = parseInt($("#target8").val()) || 0;
            var i = parseInt($("#target9").val()) || 0;
            var j = parseInt($("#target10").val()) || 0;
            var numberoftargets = parseInt($("#numberoftargets").val()) || 0;
            var max_targetscore = "5";
            var max_overallscore = "70";
            var thisVCscore = parseInt($("#theVCscore").val()) || 0;

            var numerator = a + b + c + d + e + f + g + h + i + j;
            var denominator = parseInt(numberoftargets, 10) * parseInt(max_targetscore, 10)
            var division = parseInt(numerator, 10) / parseInt(denominator, 10);

            var result = parseInt(division, 10) * parseInt(max_overallscore, 10);

            $("#thePTscore").val(result);

            var overallresult = thisVCscore + result;

            $("#theOverallscore").val(overallresult);
        })
</script>
